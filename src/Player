
public class Player extends Pawn {

	public Player(int posX, int posY, int walls) {
		super(posX, posY, walls);

	}

	@Override
	public void setWalls(int set) {

		super.setWalls(set);
	}

	@Override
	public int getWalls() {

		return super.getWalls();
	}

	@Override
	public Position getPos() {

		return super.getPos();
	}

	@Override
	public void setX(int set) {

		super.setX(set);
	}

	@Override
	public void setY(int set) {

		super.setY(set);
	}

	@Override
	public int getX() {

		return super.getX();
	}

	@Override
	public int getY() {

		return super.getY();
	}

	@Override
	public void moveX(int move) {

		super.moveX(move);
	}

	@Override
	public void moveY(int move) {

		super.moveY(move);
	}

}
