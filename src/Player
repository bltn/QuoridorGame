/**
 * Models a player.
 * 
 * @author Jordan Bird,Hoang Thai Khanh Con
 * @version 2/15/2015
 */


public class Player extends Pawn {

	public Player(int posX, int posY) {
		super(posX, posY);

	}

	



	@Override
	public void setX(int set) {

		super.setX(set);
	}

	@Override
	public void setY(int set) {

		super.setY(set);
	}

	@Override
	public int getX() {

		return super.getX();
	}

	@Override
	public int getY() {

		return super.getY();
	}

	@Override
	public void moveX(int move) {

		super.moveX(move);
	}

	@Override
	public void moveY(int move) {

		super.moveY(move);
	}

}
